<!-- Minimal STM32CubeMX .ioc configuration for Nucleo-F401RE\n     - System clock: HSI -> PLL to 84 MHz\n     - I2C1 on PB6/PB7\n     - PA0 as GPIO (DHT data)\n     - FreeRTOS (CMSIS-RTOS2) enabled\n  Note: This is a minimal .ioc. Import in STM32CubeIDE and adjust if needed.\n-->\n<STM32CubeMX>\n  <Project>\n    <ProjectName>my-weather</ProjectName>\n    <ToolVersion>6.7.0</ToolVersion>\n    <Board>Nucleo-F401RE</Board>\n    <MCU>STM32F401RETx</MCU>\n  </Project>\n  <ClockConfiguration>\n    <HSI>16000000</HSI>\n    <SYSCLK>84000000</SYSCLK>\n    <PLL>\n      <PLLM>16</PLLM>\n      <PLLN>336</PLLN>\n      <PLLP>4</PLLP>\n      <PLLQ>7</PLLQ>\n    </PLL>\n  </ClockConfiguration>\n  <Pinout>\n    <Pin name="PB6" function="I2C1_SCL"/>\n    <Pin name="PB7" function="I2C1_SDA"/>\n    <Pin name="PA0" function="GPIO_OUTPUT"/>\n  </Pinout>\n  <Peripherals>\n    <I2C1 enabled="YES">\n      <Mode>I2C</Mode>\n      <Timing>0x20404768</Timing>\n    </I2C1>\n  </Peripherals>\n  <Middleware>\n    <FREERTOS enabled="YES">\n      <API>CMSIS-RTOS2</API>\n    </FREERTOS>\n  </Middleware>\n</STM32CubeMX>